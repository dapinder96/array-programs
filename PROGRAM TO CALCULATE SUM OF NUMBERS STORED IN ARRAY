#include<stdio.h>  // sum of elements of array
int main()
{
    int size, a[100], i, sum = 0;
    printf("enter size of array = ");
    scanf("%d",&size);
    printf("%d elements of array\n", size);
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<size;i++)
    {
        sum = sum + a[i];
    }
    printf("sum of all the elements of array = %d",sum);

    
    return 0;
}
