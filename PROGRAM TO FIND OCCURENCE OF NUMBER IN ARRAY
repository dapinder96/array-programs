#include<stdio.h>
int main(){
    int size,count=0,a[100],i;
    printf("enter the size of array:");
    scanf("%d",&size);
    for(i=0;i<size;i++){
        scanf("%d",&a[i]);
    }
    int n;
    printf("enter the number in array:");
    scanf("%d",&n);
    for(i=0;i<size;i++){
        if(a[i]==n){
            count++;
        }
    }
    printf("occurence of array=%d",n,count);

}
