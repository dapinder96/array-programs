#include<stdio.h>  // positive elements in array
int main()
{
    int size, a[100], i;
    printf("enter size of array = ");
    scanf("%d",&size);
    printf("enter elements of array\n");
    for(i=0;i<size;i++)
    {
        scanf("%d", &a[i]);
    }
    printf("positive elements in array are\n");
    for(i=0;i<size;i++)
    {
        if(a[i]>0)
        {
            printf("%d ", a[i]);
        }

    }
    return 0;
}
