#include<stdio.h>  // sum of element in array
int main() 
{
    int size, i, sum = 0, a[100];
    printf("enter size of array = ");
    scanf("%d",&size);
    printf("enter elements of array\n");
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);
    }

    for(i=0;i<size;i++)
    {
        sum = sum + a[i];
    }
    printf("sum of elements of array = %d", sum);
    return 0;
}
