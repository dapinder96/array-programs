#include<stdio.h>  // sort the array in ascending order
int main()
{
    int size, a[100], i, j, m;
    printf("enter size of array: ");
    scanf("%d",&size);
    printf("enter elements of array\n");
    for(i=0;i<size;i++)
    {
        scanf("%d", &a[i]);
    }
    for(i=0;i<size;i++)
    {
        for(j=i+1;j<size;j++)
        {
            if(a[i]>a[j])
            {
                m = a[i];s
                a[i] = a[j];
                a[j] = m;
            }
        }
    }
    printf("elements arranged in ascending order\n");
    for(i=0;i<size;i++)
    {
        printf("%d ", a[i]);
    }
    return 0;
}
